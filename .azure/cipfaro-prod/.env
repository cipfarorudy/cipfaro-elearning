# Azure Developer CLI Environment Variables
# These will be set automatically by 'azd init' and 'azd provision'

# Core Azure settings
AZURE_ENV_NAME=cipfaro-prod
AZURE_LOCATION=francecentral
AZURE_SUBSCRIPTION_ID=

# Service endpoints (populated after deployment)
WEB_APP_URL=
API_APP_URL=
WEB_APP_NAME=
API_APP_NAME=

# Database configuration
POSTGRESQL_SERVER_NAME=
DATABASE_URL=

# Storage configuration  
STORAGE_ACCOUNT_NAME=
S3_ENDPOINT=
S3_BUCKET=scorm-packages

# Security
KEY_VAULT_NAME=
MANAGED_IDENTITY_ID=

# Monitoring
APPLICATION_INSIGHTS_CONNECTION_STRING=

# Domain configuration (manual setup)
CUSTOM_DOMAIN=cipfaro.fr
CORS_ORIGIN=https://cipfaro.fr